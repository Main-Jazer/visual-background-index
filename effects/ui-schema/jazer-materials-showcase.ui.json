{
  "version": 1,
  "title": "Materials Showcase",
  "defaults": {
    "timeScale": 1
  },
  "hud": {
    "title": "Materials Showcase \u2014 Controls",
    "toggleKey": "h",
    "visible": false
  },
  "hotkeys": [
    {
      "key": "H",
      "label": "Toggle Help"
    },
    {
      "key": "R",
      "label": "Reset",
      "action": "reset"
    },
    {
      "keys": [
        "1",
        "2"
      ],
      "label": "Time",
      "bind": "params.timeScale",
      "step": 0.1,
      "min": 0.1,
      "max": 3,
      "format": "fixed1"
    }
  ],
  "controls": [
    {
      "type": "slider",
      "label": "Time Scale",
      "bind": "params.timeScale",
      "min": 0,
      "max": 3,
      "step": 0.01
    }
  ],
  "hint": "Press H for help. Use JAZER_EXPOSE to expose materials and uniforms."
}