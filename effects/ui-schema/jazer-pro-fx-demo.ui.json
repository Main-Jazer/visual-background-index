{
  "version": 1,
  "title": "Pro FX Demo",
  "defaults": {
    "timeScale": 1,
    "particleCount": 200,
    "attractStrength": 0.3,
    "noiseStrength": 0.2,
    "connectionDistance": 150,
    "mouseEnabled": true
  },
  "hud": {
    "title": "Pro FX Demo \u2014 Controls",
    "toggleKey": "i",
    "visible": false
  },
  "hotkeys": [
    {
      "key": "H",
      "label": "Toggle Help"
    },
    {
      "key": "R",
      "label": "Reset",
      "action": "reset"
    },
    {
      "keys": [
        "1",
        "2"
      ],
      "label": "Time",
      "bind": "params.timeScale",
      "step": 0.1,
      "min": 0.1,
      "max": 3,
      "format": "fixed1"
    },
    {
      "key": "M",
      "label": "Mouse",
      "bind": "params.mouseEnabled",
      "type": "toggle",
      "format": "onoff"
    },
    {
      "keys": [
        "3",
        "4"
      ],
      "label": "Attractstrength",
      "bind": "params.attractStrength",
      "step": 0.1,
      "format": "fixed1",
      "min": 0,
      "max": 2
    },
    {
      "keys": [
        "5",
        "6"
      ],
      "label": "Connectiondistance",
      "bind": "params.connectionDistance",
      "step": 0.1,
      "format": "fixed1",
      "min": 0,
      "max": 2
    },
    {
      "keys": [
        "7",
        "8"
      ],
      "label": "Noisestrength",
      "bind": "params.noiseStrength",
      "step": 0.1,
      "format": "fixed1",
      "min": 0,
      "max": 2
    },
    {
      "keys": [
        "9",
        "10"
      ],
      "label": "Particlecount",
      "bind": "params.particleCount",
      "step": 10,
      "format": "int",
      "min": 10,
      "max": 500
    }
  ],
  "controls": [
    {
      "type": "slider",
      "label": "Time Scale",
      "bind": "params.timeScale",
      "min": 0,
      "max": 3,
      "step": 0.01
    },
    {
      "type": "checkbox",
      "label": "Mouse Enabled",
      "bind": "params.mouseEnabled"
    },
    {
      "type": "slider",
      "label": "Attractstrength",
      "bind": "params.attractStrength",
      "step": 0.01,
      "min": 0,
      "max": 2
    },
    {
      "type": "slider",
      "label": "Connectiondistance",
      "bind": "params.connectionDistance",
      "step": 0.01,
      "min": 0,
      "max": 2
    },
    {
      "type": "slider",
      "label": "Noisestrength",
      "bind": "params.noiseStrength",
      "step": 0.01,
      "min": 0,
      "max": 2
    },
    {
      "type": "slider",
      "label": "Particlecount",
      "bind": "params.particleCount",
      "step": 1,
      "min": 10,
      "max": 500
    },
    {
      "type": "color",
      "label": "Colors 1",
      "bind": "effect.colors[0]"
    },
    {
      "type": "color",
      "label": "Colors 2",
      "bind": "effect.colors[1]"
    }
  ],
  "hint": "Press H for help. Canvas 2D effect."
}