{
  "version": 1,
  "title": "Radiant Pulse Rings",
  "defaults": {
    "timeScale": 1,
    "mouseEnabled": true,
    "bgColor": "#000000",
    "ringCount": 24,
    "pulseSpeed": 1,
    "pulseAmp": 0.4,
    "glowStrength": 1,
    "colorA": "#00f5ff",
    "colorB": "#ff2aff"
  },
  "hud": {
    "title": "Radiant Pulse Rings - Controls",
    "toggleKey": "h",
    "visible": false
  },
  "hotkeys": [
    {
      "key": "H",
      "label": "Toggle Help"
    },
    {
      "key": "R",
      "label": "Reset",
      "action": "reset"
    },
    {
      "keys": [
        "1",
        "2"
      ],
      "label": "Time",
      "bind": "params.timeScale",
      "step": 0.1,
      "min": 0.1,
      "max": 3,
      "format": "fixed1"
    }
  ],
  "controls": [
    {
      "type": "slider",
      "label": "Time Scale",
      "bind": "params.timeScale",
      "min": 0,
      "max": 3,
      "step": 0.01
    },
    {
      "type": "slider",
      "label": "Ring Count",
      "bind": "params.ringCount",
      "min": 0,
      "max": 200,
      "step": 1
    }
  ],
  "hint": "Press H for help. This effect HTML is currently a placeholder; schema is a stub."
}
